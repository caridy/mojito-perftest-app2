[{
    "settings": [ "master" ],

    "appPort": 8666,

    "shareYUIInstance": true,

    "yui": {
        "dependencyCalculations": "precomputed+ondemand"
    },

    "middleware": [
         "mojito-handler-static",
         "mojito-parser-body",
         "mojito-parser-cookies",
         "mojito-contextualizer",
         "mojito-handler-tunnel",
         "mojito-router",
         "mojito-handler-dispatcher"
    ],

    "specs": {
        "htmlframe": {
            "type": "ShakerHTMLFrameMojit",
            "config": {
                "deploy": true,
                "enableDynamicTitle": true,
                "child": {
                    "base": "master"
                }
            }
        },

        "master": {
            "type": "MasterMojit",
            "config": {
                "foo": "bar"
            }
        }
    },

    "log": {
        "server": {
            "level": "error"
        }
    },

    "shaker": {
        "routeBundle": {
            "perf-test-index": [
                "Mojit1.index",
                "Mojit2.index",
                "Mojit3.index",
                "Mojit4.index",
                "Mojit5.index",
                "Mojit6.index",
                "Mojit7.index",
                "Mojit8.index",
                "Mojit9.index",
                "Mojit10.index"
            ]
        },
        "task": "raw",
        "module": null,
        "taskConfig": null
    },

    "mojitsDirs": [
        "mojits",
        "node_modules/mojito-shaker/mojits"
    ]
}]
